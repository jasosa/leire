package cat.hmobile.leire.business.microblogging.statusnetextension;

public class TimeAgoStatus {
	private long m_Days;
	private long m_Hours;
	private long m_Minutes;
	
	public TimeAgoStatus(long days, long hours, long minutes){
		m_Days = days;
		m_Hours = hours;
		m_Minutes = minutes;
	}
	
	public long getDays() {
		return m_Days;
	}
	
	public long getHours() {
		return m_Hours;
	}
	
	public long getMinutes() {
		return m_Minutes;
	}
}
