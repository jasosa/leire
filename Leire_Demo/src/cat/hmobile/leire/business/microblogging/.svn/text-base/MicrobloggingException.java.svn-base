package cat.hmobile.leire.business.microblogging;

import cat.hmobile.leire.entities.LeireException;

public class MicrobloggingException extends LeireException {
	
	String m_message;
	
	public MicrobloggingException(int errorMessageCode){
		m_errorMessageCode = errorMessageCode;
	}
	
	public MicrobloggingException(String message){
		m_message = message;
	}
	
	public MicrobloggingException() {
	}

	@Override
	public String getMessage() {
		return m_message;
	}
	

	
}
